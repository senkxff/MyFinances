<Page x:Class="MyFinances.View.Pages.WalletsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MyFinances.View.Pages"
      xmlns:uc="clr-namespace:MyFinances.View.UserControls.WalletsPageButtons"
      mc:Ignorable="d" 
      Height="550" Width="750"
      Title="WalletsPage">

    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 0 0 150">
            <uc:CashButton VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10"/>
            <uc:CardButton VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10 10 10 10"/>
        </StackPanel>
    </Grid>
</Page>
